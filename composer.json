{
    "name": "clearbooks/dilex",
    "description": "Silex with enhanced DI",
    "type": "project",
    "require": {
        "silex/silex": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "clearbooks/di-container-mock": "1.0.0"
    },
    "license": "LGPL-v3",
    "authors": [
        {
            "name": "Crimson Ducks",
            "email": "support@clearbooks.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:clearbooks/di-container-mock.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "test/"
        }
    },
    "minimum-stability": "stable"
}
