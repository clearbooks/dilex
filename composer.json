{
    "name": "clearbooks/dilex",
    "description": "Symfony with enhanced DI and a Silex-like interface",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "psr/container": "^1.0",
        "symfony/http-kernel": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/routing": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/dependency-injection": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Crimson Ducks",
            "email": "support@clearbooks.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "test/"
        }
    },
    "minimum-stability": "stable"
}
