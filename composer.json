{
    "name": "clearbooks/dilex",
    "description": "Silex with enhanced DI",
    "type": "project",
    "require": {
        "silex/silex": "1.3",
        "tomverran/di-container-mock": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "tomverran/di-container-mock": "*"
    },
    "license": "LGPL-v3",
    "authors": [
        {
            "name": "Crimson Ducks",
            "email": "support@clearbooks.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://carefulcoder.co.uk/composer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clearbooks\\Dilex\\": "test/"
        }
    },
    "minimum-stability": "dev"
}
